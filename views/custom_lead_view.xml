<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_custom_lead_form" model="ir.ui.view">
        <field name="name">custom.lead.form</field>
        <field name="model">crm.lead</field>
        <field name="arch" type="xml">
            <form string="Custom Lead Form">
                <!-- Define your view layout here -->
                <!-- Example: Display fields like name, stage_id, etc. -->
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="stage_id"/>
                        <field name="team_id"/> <!-- Ensure team_id is included -->
                        <!-- Add more fields as needed -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
